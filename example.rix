Point :: BaseType
  x = int
  y = int
  
  $(int x, int y)
    $.x=x
    $.y=y

Line :: BaseType
  a = Point
  b = Point
  
  $(Point a, Point b)
    $.a=a
    $.b=b

  length->double() = ((((a.x-b.x)^^2) + ((a.y-b.y)^^2)) ^^ 0.5)

