Boolean : isPrime Integer n
    (n < 2) if
        -> false
    (n == 2) if
        -> true
    (n % 2 == 0) if
        -> false
    upperLimit = (n sqrt)
    i = 3
    i <= upperLimit while
        (n % i == 0) if
            -> false
        i += 2
    -> true


lim = 2000000
numPrimes = 0
n = 0

i for 1, lim
    (isPrime i) if
        numPrimes = (numPrimes + 1)

print "Number of primes in the interval [0,"+lim+"]: "+numPrimes
